{
  "name": "securet-flow-ssc-frontend",
  "version": "1.0.0",
  "description": "Securet Flow SSC - Frontend Application",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "vite",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "security",
    "vulnerability-assessment",
    "penetration-testing",
    "cybersecurity",
    "purple-team",
    "react",
    "typescript",
    "vite"
  ],
  "author": "Securet Flow SSC Team",
  "license": "MIT",
  "dependencies": {
    "@headlessui/react": "^1.7.19",
    "@heroicons/react": "^2.0.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tanstack/react-query": "^4.29.0",
    "axios": "^1.4.0",
    "chart.js": "^4.5.0",
    "clsx": "^1.2.1",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.12.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.0",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^6.8.0",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^3.3.0",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.40.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.0",
    "jsdom": "^21.1.0",
    "postcss": "^8.4.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "typescript": "^5.0.0",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vmzzt/securetflow.git"
  },
  "bugs": {
    "url": "https://github.com/vmzzt/securetflow/issues"
  },
  "homepage": "https://github.com/vmzzt/securetflow#readme"
}
